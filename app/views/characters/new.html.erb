<% provide(:title, "Character") %>
<h1>Character</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(:character, url: '/character') do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit "Send", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>